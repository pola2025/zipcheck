{
  "name": "zipcheck-backend",
  "version": "1.0.0",
  "description": "ZipCheck 데이터 관리 백엔드",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
